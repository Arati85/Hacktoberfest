#include<bits/stdc++.h>
using namespace std;

string solve(string s){
    if(s.size()<5){
        return "Too weak";
    }
    bool digit=false,upper=false,lower=false,special=false;
    for(int i=0;i<s.size();i++){
        if(isdigit(s[i])){
            digit=true;
        }
        else if(islower(s[i])){
            lower=true;
        }
        else{
            upper=true;
        }
    }
    if(digit && upper && lower){
        return "Correct";
    }
    else{
        return "Too weak";
    }
}
int main(){
    string s;
    cin>>s;
    cout<<solve(s);
    return 0;
}
